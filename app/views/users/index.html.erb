<header>
<h1>Users (<%= User.all.count %>)</h1>
</header>
<div class="container">
  <div class="row">
    <div class="column">
      <table class="table">
        <tbody>
          <% @users.each do |user| %>
            <tr>
              <%= render user %>
            </tr>
          <% end %>
        </tbody>
        <%= paginate @users, :html => {:class => "pagination"} %>
      </table>
    </div>
  </div>
</div>
